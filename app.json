{
  "expo": {
    "name": "Flappy Bird",
    "slug": "flappy-bird",
    "plugins": ["sentry-expo"],
    "version": "0.0.1",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "$SENTRY_ORG",
            "project": "$SENTRY_PROJECT",
            "authToken": "$SENTRY_AUTH_TOKEN"
          }
        }
      ]
    }
  }
}
